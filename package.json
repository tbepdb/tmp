{
  "name": "sitoserver",
  "version": "0.0.0",
  "description": "Backend for sitochat",
  "main": "dist/src/app.js",
  "scripts": {
    "cb": "grunt lint build",
    "lintcmd": "tslint -t verbose --project tsconfig.json --config tslint.json --exclude typings src/*.ts",
    "lint": "grunt tslint:check",
    "lintfix": "grunt tslint:fix",
    "build": "grunt build",
    "start": "NODE_PATH='./dist/src' node dist/src/app",
    "postinstall": "typings install && grunt build",
    "onetest": "grunt ts:onetest && NODE_PATH='./dist/src' node dist/src/test",
    "test": "NODE_PATH='./dist/src' grunt test",
    "dockerpush": "cd docker && DATE=`date +%Y-%m-%d` && BRANCH=`git rev-parse --abbrev-ref HEAD` && REV=`git rev-parse HEAD` && docker build -t sitochat/server:webserver.b2e.${BRANCH}-${DATE} --build-arg BRANCH=${BRANCH} --build-arg REV=${REV} ./ && docker push sitochat/server:webserver.b2e.${BRANCH}-${DATE}"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:sitochat/sitoserver.git"
  },
  "author": "sitochat",
  "license": "",
  "devDependencies": {
    "@types/chai": "~3.4.35",
    "@types/node": "~8.0.19",
    "@types/sinon": "~1.16.35",
    "chai": "~3.5.0",
    "grunt": "~1.0.1",
    "grunt-cli": "~1.2.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-mocha-test": "~0.13.2",
    "grunt-ts": "~6.0.0-beta.11",
    "grunt-tslint": "~5.0.1",
    "mocha": "~3.1.2",
    "mocha-typescript": "~1.0.22",
    "pm2": "~2.5.0",
    "sinon": "~1.17.7",
    "tslint": "~5.8.0",
    "typescript": "~2.5.1",
    "typings": "~2.1.0"
  },
  "dependencies": {
    "apiai": "~4.0.3",
    "block-sequence": "~1.0.0",
    "block-sequence-redis": "~1.1.0",
    "block-sequence-reference": "~0.1.0",
    "cerialize": "~0.1.16",
    "elastic-builder": "^1.1.0",
    "elasticsearch": "~13.3.1",
    "express": "~4.14.0",
    "fcm-node": "~1.0.16",
    "generic-pool": "~3.1.4",
    "gm": "~1.23.0",
    "int64-buffer": "~0.1.9",
    "log4js": "~1.0.1",
    "nodemailer": "~1.3.2",
    "object-assign": "~4.1.0",
    "object-path": "~0.11.3",
    "open-graph-scraper": "~2.5.0",
    "pug": "~2.0.0-beta6",
    "reflect-metadata": "~0.1.8",
    "semaphore-async-await": "^1.5.1",
    "socket.io": "~1.6.0",
    "sphinxapi": "~1.2.1",
    "tarantool-driver": "~2.0.1",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.3.0"
  }
}
